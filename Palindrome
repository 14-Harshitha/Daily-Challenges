#include<stdio.h>
int main()
{
    int n,num=0,r=0,rev=0;
    scanf("%d",&n);
    num=n;
    while(n!=0)
    {
        r=n%10;
        rev=rev*10+r;
        n=n/10;
    }
    if(rev==num)
    {
        printf("Palindrome");
    }
    else
    {
        printf("Not Palindrome");
    }
    
    return 0;
}
